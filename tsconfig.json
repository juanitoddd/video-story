{
	"compilerOptions": {
		"outDir": "./dist",
		"target": "ES2018",
		"module": "commonjs",
		"jsx": "react-jsx",
		"strict": true,
		"lib": ["es2015", "DOM"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"noUnusedLocals": true
	}
}
